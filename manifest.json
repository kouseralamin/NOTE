{
  "name": "NOTE",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "description": "Take quick note in New Tab & in PopUp Window.",
  "action": {
    "default_title": "NOTE",
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "manifest/icons/16.png",
      "32": "manifest/icons/32.png",
      "192": "manifest/icons/192.png",
      "512": "manifest/icons/512.png"
    }
  },
  "icons": {
    "16": "manifest/icons/16.png",
    "32": "manifest/icons/32.png",
    "192": "manifest/icons/192.png",
    "512": "manifest/icons/512.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab/index.html"
  }
}